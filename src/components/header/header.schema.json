  "type": "object",
  "properties": {
    "logo": {
      "$ref": "http://schema.kickstartds.com/base/picture.definitions.json"
    },
    "logoHref": {
      "type": "string",
      "default": "/"
    },
    "navItems": {
      "$ref": "http://schema.mydesignsystem.com/nav-main.schema.json#/properties/items"
    }
  },
  "additionalProperties": false,
  "required": ["logo"]
}
